package egovframework.example.main.web;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
public class MainController {
	
	@RequestMapping(value = "/main.do")
	public String initMain() throws Exception {
		return "main/main.adminTiles";
	}
	
	@RequestMapping(value = "/login.do")
	public String initLogin() throws Exception {
		return "login/login.loginTiles";
	}
	
	@RequestMapping(value = "/UserPageUIConfig.do")
	public String initUIConfig() throws Exception {
		
		return "main/userUiConfig";
	}
}
