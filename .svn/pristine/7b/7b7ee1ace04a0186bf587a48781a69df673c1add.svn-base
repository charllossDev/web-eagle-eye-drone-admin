<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE tiles-definitions PUBLIC 
        "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" 
        "http://tiles.apache.org/dtds/tiles-config_3_0.dtd"> 
 
<tiles-definitions>
	<definition name="layouts-tiles" 	 template="/WEB-INF/jsp/egovframework/example/cmmn/default-layouts.jsp">
	  	<put-attribute name="head"    		value="/WEB-INF/jsp/egovframework/example/cmmn/default-head.jsp" />
	  	<put-attribute name="header"    	value="/WEB-INF/jsp/egovframework/example/cmmn/default-header.jsp" />
	  	<put-attribute name="leftMenu"    	value="/WEB-INF/jsp/egovframework/example/cmmn/default-leftMenu.jsp" />
	  	<put-attribute name="tab"    		value="/WEB-INF/jsp/egovframework/example/cmmn/default-tab.jsp" />
	  	<put-attribute name="content"   	value="/WEB-INF/jsp/egovframework/example/cmmn/default-content.jsp" />
	  	<put-attribute name="rightMenu"   	value="/WEB-INF/jsp/egovframework/example/cmmn/default-rightMenu.jsp" />
	  	<put-attribute name="footer"    	value="/WEB-INF/jsp/egovframework/example/cmmn/default-footer.jsp" />
	  	<put-attribute name="script"    	value="/WEB-INF/jsp/egovframework/example/cmmn/default-script.jsp" />
	</definition>
	
	<definition name="*/*.adminTiles" extends="layouts-tiles">

	</definition>
	
	<definition name="login-tiles" 		 template="/WEB-INF/jsp/egovframework/example/login/login-layouts.jsp">
	  	<put-attribute name="head"    		value="/WEB-INF/jsp/egovframework/example/login/login-head.jsp" />
	  	<put-attribute name="content"   	value="" />
	</definition>
	
	<definition name="*/*.loginTiles" extends="login-tiles">
	  	<put-attribute name="content"   value="/WEB-INF/jsp/egovframework/example/{1}/{2}.jsp" />
	</definition>
	
</tiles-definitions>